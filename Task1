num = 100
while (num >= 2):
 print(num)
 num -= 2

newNum = int(input("Please enter a number: "))
print("Multiplication table of ", newNum)
for i in range(1, 11):
        print(newNum, "X", i, "=", newNum * i)

myName = []
counter = 1
while counter<11:
    strName= input("Please enter name: ")
    myName.append(myName)
    counter += 1
while counter<11:
    print(myName)